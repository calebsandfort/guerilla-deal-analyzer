{
  "name": "guerilla-deal-analyzer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run build:frontend && npm run build:backend",
    "lint": "vue-cli-service lint",
    "deploy": "npm run build && npm run deploy:frontend && npm run deploy:backend",
    "babel-build:backend": "babel src/backend -d dist_backend/",
    "build:backend": "npm run remove:backend && npm run babel-build:backend && npm run copy:backend",
    "build:frontend": "vue-cli-service build",
    "copy:backend": "cp package.json dist_backend/ && cp .env dist_backend/",
    "deploy:backend": "pm2 deploy ecosystem.config.js production",
    "deploy:frontend": "vue-cli-service s3-deploy",
    "remove:backend": "rm -rf dist_backend/*",
    "rollup:backend": "npm run remove:backend && ./node_modules/.bin/rollup -c && npm run copy:backend",
    "start:backend": "nodemon --exec babel-node src/backend/index.js",
    "test:mocha": "mocha --reporter spec --require @babel/register --recursive \"src/**/*.spec.js\"",
    "test:mocha:debug": "mocha --reporter spec --inspect-brk --require @babel/register --recursive \"src/**/*.spec.js\"",
    "test:unit": "vue-cli-service test:unit",
    "test:unit:debug": "node --inspect-brk ./node_modules/.bin/vue-cli-service test:unit --no-cache --runInBand",
    "test:all": "npm run test:mocha && npm run test:unit --no-cache --runInBand",
    "webpack:backend": "npm run remove:backend && webpack && npm run copy:backend"
  },
  "dependencies": {
